<h1><%= @book.title %></h1>
<img src=<%= @book.image%>>
<h3> Author </h3>
<p><%= @book.author %></p>
<h3> Price </h3>
<p>$<%= @book.price %></p>
<h3> Publisher </h3>
<p><%= @book.publisher %></p>
<h3> Date Published</h3>
<p><%= @book.date_published %></p>
<h3> Genre </h3>
<p><%= @book.genre %></p>
<button type="button" class="btn btn-primary-outline">
<a href="/books/<%= @book.id %>/edit">Update</a>
</button>
<%= link_to "Delete this book.", "/books/#{@book.id}", method: :delete %>