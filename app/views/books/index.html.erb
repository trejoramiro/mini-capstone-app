<h1>Books</h1>
<button type="button" class="btn btn-primary-outline">
<a href="/books/new">Create Book</a>
</button>
<% @books.each do |book| %>
<h2> <a href="/books/<%= book.id %>"> <%= book.title %> </a></h2>
<img src=<%= book.image %> >
<h3> Author </h3>
<p><%= book.author %></p>
<h3> Price </h3>
<p>$<%= book.price %></p>
<h3> Publisher </h3>
<p><%= book.publisher %></p>
<h3> Date Published</h3>
<p><%= book.date_published %></p>
<h3>Genre</h3>
<p><%= book.genre %></p>

<% end %>